{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "allowParens": "always",
  "useTabs": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "singleAttributePerLine": true,
  "printWidth": 80,
  "endOfLine": "auto"
}
